{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "address": {
      "type": "text",
      "required": true
    },
    "institute": {
      "type": "string",
      "required": true
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "male",
        "female"
      ],
      "default": "male",
      "required": true
    },
    "point": {
      "type": "integer",
      "default": 0,
      "required": true
    },
    "designation": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "facebookUrl": {
      "type": "string",
      "required": false,
      "unique": true
    },
    "requests": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::request.request"
    },
    "feedbacks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::feedback.feedback"
    },
    "enabled": {
      "type": "boolean",
      "default": false
    },
    "subjects": {
      "type": "component",
      "repeatable": true,
      "component": "array.single",
      "required": true
    },
    "locations": {
      "type": "component",
      "repeatable": true,
      "component": "array.single",
      "required": true
    },
    "method": {
      "type": "enumeration",
      "enum": [
        "offline",
        "online"
      ],
      "default": "offline",
      "required": true
    }
  }
}
